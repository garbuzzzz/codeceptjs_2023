{
	"name": "codeceptjs_2023",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
			"prepare": "rm -r -f output/* && rm -r -f allure-results/*",
			"test": "codeceptjs run --steps --reporter mocha-multi",
			"codeceptjs": "codeceptjs run --steps",
			"codeceptjs:headless": "HEADLESS=true npx codeceptjs run --steps",
			"codeceptjs:ui": "npx codecept-ui --app",
			"codeceptjs:demo": "codeceptjs run --steps -c node_modules/@codeceptjs/examples",
			"codeceptjs:demo:headless": "HEADLESS=true codeceptjs run --steps -c node_modules/@codeceptjs/examples",
			"codeceptjs:demo:ui": "npx codecept-ui --app  -c node_modules/@codeceptjs/examples",
			"codeceptjs:dry-run": "npx codeceptjs dry-run",
			"codeceptjs:allure": "npx codeceptjs run --plugins allure",
			"allure:serve": "npx allure serve",
			"codeceptjs:pause": "codeceptjs run --steps -p pauseOnFail",
			"multiple:threebrowsers": "npm run prepare && npx codeceptjs run-multiple --steps threebrowsers --plugins allure",
			"parallel:chromeonly": "npm run prepare && npx codeceptjs run-multiple --steps chromeonly --plugins allure --reporter mocha-multi",
			"parallel:firefoxonly": "npm run prepare && npx codeceptjs run-multiple --steps firefoxonly --plugins allure"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
			"@codeceptjs/allure-legacy": "^1.0.2",
			"@codeceptjs/configure": "^0.10.0",
			"@codeceptjs/examples": "^1.2.1",
			"@codeceptjs/ui": "^0.4.7",
			"allure-commandline": "^2.22.0",
			"codeceptjs": "^3.4.1",
			"eslint": "^8.40.0",
			"eslint-config-prettier": "^8.8.0",
			"eslint-plugin-codeceptjs": "^1.3.0",
			"husky": "^8.0.3",
			"lint-staged": "^13.2.2",
			"mocha-junit-reporter": "^2.2.0",
			"mocha-multi": "^1.1.7",
			"playwright": "^1.33.0",
			"prettier": "^2.8.8"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run unit:test"
		}
	}
}
